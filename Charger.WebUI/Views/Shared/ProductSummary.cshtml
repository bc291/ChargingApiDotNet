@model Charger.Domm.Entities.ChOperation

<div class="well">
   
    <h3>      
        <div class="row">
            <div class="col-sm-8"> <strong>@Model.carModel</strong></div>
            <div class="col-sm-1 col-md-offset-1 label label-primary">@Model.cost.ToString("c")</div>
            <div class="col-sm-1 col-md-offset-1 label label-primary">@Model.averagePower kWh</div>
        </div> 
        
    </h3>
    <span class="lead">
        @Model.dateAndTime
        <div class="row">
            <div class="col-sm-8"> </div>
            <div class="col-sm-1 col-md-offset-1 label label-primary">@Model.elapsedTime min</div>
            <div class="col-sm-1 col-md-offset-1 label label-primary">@Model.initialCapacity kWh</div>
        </div>
    </span>
</div>